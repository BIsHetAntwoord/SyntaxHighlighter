				<Option type="1" />
				<Option compiler="gcc" />
				<Option parameters="input.S assembly" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fexceptions" />
		</Compiler>
		<Unit filename="asmlexer.l">
			<Option compile="1" />
		</Unit>
		<Unit filename="cpplexer.l">
			<Option compile="1" />
		</Unit>
		<Unit filename="highlighter.cpp" />
		<Unit filename="highlighter.h" />
		<Unit filename="javalexer.l">
			<Option compile="1" />
		</Unit>
		<Unit filename="lexer.cpp" />
		<Unit filename="main.cpp" />
		<Unit filename="scanners.h" />
		<Extensions>
			<code_completion />
			<debugger />
			<envvars />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
